<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remove-project name="crdroidandroid/android" />
  <project path="android" name="crDroid-Revived/android" remote="crdroid" />

  <remove-project name="crdroidandroid/android_build" />
  <project name="crDroid-Revived/android_build" path="build/make" remote="crdroid" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>

  <remove-project name="crdroidandroid/android_frameworks_av" />
  <project name="crDroid-Revived/android_frameworks_av" path="frameworks/av" remote="crdroid" />
  <remove-project name="crdroidandroid/android_frameworks_base" />
  <project name="crDroid-Revived/android_frameworks_base" path="frameworks/base" remote="crdroid" />

  <remove-project name="LineageOS/android_system_bt" />
  <project name="LineageOS-Revived/android_system_bt" path="system/bt" />

  <remove-project name="crdroidandroid/android_vendor_qcom_opensource_system_bt" />
  <project name="crDroid-Revived/android_vendor_qcom_opensource_system_bt" path="vendor/qcom/opensource/commonsys/system/bt" remote="crdroid" />
</manifest>
